{
  "name": "el-bot-js",
  "version": "0.0.1",
  "description": "A quick qq bot for mirai.",
  "main": "index.ts",
  "scripts": {
    "dev": "export NODE_ENV=dev && nodemon index.ts",
    "install:mirai": "node scripts/install-mirai.js",
    "lint": "eslint src --ext .ts",
    "start": "nodemon index.ts",
    "start:console": "node scripts/start-mirai.js",
    "start:webhook": "nodemon scripts/webhook.js",
    "test": "npm run lint",
    "update:config": "cd config/custom && git add -A && git commit -m ':wrench: update config' && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elpsycn/el-bot-js.git"
  },
  "keywords": [
    "elpsy",
    "bot",
    "mirai",
    "qq"
  ],
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https:/www.yunyoujun.cn"
  },
  "contributors": [
    "@YunYouJun",
    "@ADD-SP"
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/elpsycn/el-bot-js/issues"
  },
  "homepage": "https://github.com/elpsycn/el-bot-js",
  "dependencies": {
    "chalk": "^4.0.0",
    "dayjs": "^1.8.28",
    "dotenv": "^8.2.0",
    "feedparser": "^2.2.10",
    "follow-redirects": "^1.11.0",
    "github-webhook-handler": "^1.0.0",
    "glob": "^7.1.6",
    "html-to-text": "^5.1.1",
    "js-yaml": "^3.14.0",
    "mirai-ts": "./packages/mirai-ts",
    "node-mirai-sdk": "^0.1.26",
    "node-schedule": "^1.3.2",
    "rss-parser": "^3.8.0",
    "shelljs": "^0.8.4",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
